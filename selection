#include <iostream>
#include <stdlib.h>     /* rand */
#include<climits>

using namespace std;

// A utility function to swap two elements
void swap(int *x, int *y)
{
    int temp = *x;
    *x = *y;
    *y = temp;
}

// A utility function to generate dataset
int * generate_dataset (int min , int max ,int n)   // n is the size of the dataset
{
    int *dataset = (int *)malloc(sizeof(int)*n)  , data ;
    for (int i = 0; i < n; i++) {
        data = rand() % max + min;                  //generate random number
        dataset[i]=data;                            //add to the array
    }
    return dataset;
}

void insertion_sort(int *arr,int size )
{
    int temp,j;
    for(int i=0; i<size; i++)
    {
        for(j=i+1; j<size; j++)
        {
            if(arr[i]>arr[j])
            {
                temp=arr[i];
                arr[i]=arr[j];
                arr[j]=temp;
            }
        }
    }
}
int main() {

    /* Generate dataset*/
    int min=1,max=1000,n=200;
    int * dataset= generate_dataset(min,max,n);
    /* Sort */
    insertion_sort(dataset , n );
    /* Print */
    for (int i = 0; i <n - 1; i++) {
        cout<<dataset[i]<<endl;
    }
    return 0;
}
